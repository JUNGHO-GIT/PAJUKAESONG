{
  "name": "pajukaesong-client",
	"homepage": "/PAJUKAESONG",
	"version": "1.0.0",
	"type": "module",
	"main": "index.tsx",
	"license": "Apache-2.0",
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		],
		"rules": {
			"import/extensions": "off",
			"import/prefer-default-export": "off",
			"no-console": "off",
			"no-underscore-dangle": "off",
			"no-unused-vars": "off",
			"react-hooks/exhaustive-deps": "off",
			"react-hooks/rules-of-hooks": "warn",
			"@typescript-eslint/no-unused-expressions": "off",
			"@typescript-eslint/no-unused-vars": [
				"off",
				{
					"varsIgnorePattern": "^_",
					"argsIgnorePattern": "^_"
				}
			]
		}
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"engines": {
		"node": "16.x"
	},
	"overrides": {
		"webidl-conversions": "6.1.0",
		"whatwg-url": "12.0.1"
	},
	"scripts": {
		"start": "bun .node/swc.cjs --bun --start --client",
		"build": "bun .node/swc.cjs --bun --build --client",
		"reset": "bun .node/reset.cjs --bun --reset",
		"fix": "bun .node/fix.cjs --bun --fix",
		"gcloud": "bun .node/gcloud.cjs --bun --deploy"
	},
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.3.5",
    "@mui/system": "^7.3.5",
    "@mui/x-date-pickers": "^8.19.0",
    "@react-hooks-library/core": "^0.6.2",
    "ajv": "^8.17.1",
    "ajv-keywords": "^5.1.0",
    "axios": "^1.13.2",
    "countries-and-timezones": "^3.8.0",
    "date-fns": "^4.1.0",
    "iso-country-currency": "^0.7.2",
    "material-ui-popup-state": "^5.3.6",
    "moment": "^2.30.1",
    "moment-timezone": "^0.6.0",
    "mui-file-input": "^7.0.0",
    "react": "^19.2.0",
    "react-calendar": "^6.0.0",
    "react-dom": "^19.2.0",
    "react-router": "^7.9.6",
    "recharts": "^3.4.1",
    "swiper": "^12.0.3",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@mui/types": "^7.4.8",
		"@testing-library/react": "^16.3.0",
    "@types/navermaps": "^3.9.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@types/react-router": "^5.1.20",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "@vitejs/plugin-react-swc": "^4.2.2",
		"vite-plugin-compression": "^0.5.1"
  }
}
