{
  "name": "pajukaesong-server",
  "version": "1.1.2",
  "type": "module",
  "main": "index.ts",
  "license": "Apache-2.0",
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "import/extensions": "off",
      "import/prefer-default-export": "off",
      "no-console": "off",
      "no-underscore-dangle": "off",
      "no-unused-vars": "off",
      "react-hooks/exhaustive-deps": "off",
      "react-hooks/rules-of-hooks": "warn",
      "@typescript-eslint/no-unused-vars": [
        "off",
        {
          "varsIgnorePattern": "^_",
          "argsIgnorePattern": "^_"
        }
      ],
      "@typescript-eslint/no-unused-expressions": "off"
    }
  },
  "engines": {
    "node": "16.x"
  },
  "overrides": {
    "webidl-conversions": "6.1.0",
    "whatwg-url": "12.0.1"
  },
  "scripts": {
    "start": "bun .node/swc.cjs --bun --start --server",
    "compile": "bun .node/swc.cjs --bun --compile --server",
    "reset": "bun .node/reset.cjs --bun --reset",
    "fix": "bun .node/fix.cjs --bun --fix",
    "git": "bun .node/git.cjs --bun --push",
    "gcloud": "bun .node/gcloud.cjs --bun --deploy"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.17.3",
    "@types/react-router-dom": "^5.3.3",
    "ajv": "^8.17.1",
    "ajv-keywords": "^5.1.0",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "google-auth-library": "^10.5.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "moment-timezone": "^0.6.0",
    "mongodb": "^7.0.0",
    "mongoose": "^9.0.0",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.10",
    "oauth-signature": "^1.5.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "qs": "^6.14.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "stripe": "^20.0.0"
  },
  "devDependencies": {
    "@swc/cli": "0.7.9",
    "@swc/core": "1.15.3",
    "@swc/helpers": "0.5.17",
    "@types/bcryptjs": "^3.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/express-session": "^1.18.2",
    "@types/jsdom": "^27.0.0",
    "@types/mocha": "^10.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.1",
    "@types/nodemailer": "^7.0.4",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.17",
    "@types/qs": "^6.14.0",
    "@types/swiper": "^6.0.0",
    "jsdom": "^27.2.0",
    "tslib": "^2.8.1",
    "esbuild": "0.27.0",
    "ts-morph": "^27.0.2",
    "ts-prune": "0.10.3",
    "tsc-alias": "1.8.16",
    "typescript": "5.9.3"
  }
}
